{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "smudgy": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "css",
          "skipTests": true,
          "changeDetection": "OnPush"
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {},
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/smudgy",
      "sourceRoot": "projects/smudgy/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/smudgy",
            "index": "projects/smudgy/src/index.html",
            "main": "projects/smudgy/src/main.ts",
            "polyfills": "projects/smudgy/src/polyfills.ts",
            "tsConfig": "projects/smudgy/tsconfig.app.json",
            "preserveSymlinks": true,
            "assets": ["projects/smudgy/src/favicon.ico", "projects/smudgy/src/assets"],
            "styles": ["projects/smudgy/src/styles.css"],
            "scripts": [],
            "allowedCommonJsDependencies": ["debug"],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "customWebpackConfig": {
              "path": "webpack.config.ts"
            }
          },
          "configurations": {
            "staging": {
              "fileReplacements": [
                {
                  "replace": "projects/smudgy/src/environments/environment.ts",
                  "with": "projects/smudgy/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/smudgy/src/environments/environment.ts",
                  "with": "projects/smudgy/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "smudgy:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "smudgy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "smudgy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/smudgy/src/test.ts",
            "polyfills": "projects/smudgy/src/polyfills.ts",
            "tsConfig": "projects/smudgy/tsconfig.spec.json",
            "karmaConfig": "projects/smudgy/karma.conf.js",
            "assets": ["projects/smudgy/src/favicon.ico", "projects/smudgy/src/assets"],
            "styles": ["projects/smudgy/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/smudgy/**/*.ts", "projects/smudgy/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
